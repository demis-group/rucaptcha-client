{
  "name": "demis-group/rucaptcha-client",
  "keywords": ["php", "library", "client", "captcha recognition", "rucaptcha"],
  "description" : "This is a modern full-featured RuCaptcha PHP client based on GuzzleHttp framework.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.com",
      "homepage": "https://github.com/gladyshev"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "~6.1",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Rucaptcha\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rucaptcha\\Tests\\" : "tests/"
    }
  }
